pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo " In am doing build"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "I am doing Test"'
            }
        }
        stage('Deploy') {
            steps {
                sh ' echo "I am doing deploy"'
            }
        }
    }
}